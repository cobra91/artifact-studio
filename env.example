# AI Provider API Keys
# Copy this file to .env.local and add your actual API keys

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# AIML API Key (for GPT-5 access)
AIML_API_KEY=your_aiml_api_key_here

# OpenRouter API Key (for multiple models)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Optional: Custom API URLs
AIML_API_URL=https://api.aimlapi.com/v1
OPENROUTER_API_URL=https://openrouter.ai/api/v1

# App URL for OpenRouter
NEXT_PUBLIC_APP_URL=http://localhost:3000
